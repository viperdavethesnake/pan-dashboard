{
  "title": "User & Owner Analysis",
  "tags": [
    "file-share",
    "users",
    "owners"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "5m",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Unique Users",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT COUNT(DISTINCT owner) as count FROM file_share.file_scan WHERE is_directory = 0"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "text": {
          "titleSize": 18,
          "valueSize": 40
        },
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "blue",
                "value": null
              }
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Admin-Owned Files",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT COUNT(*) as count FROM file_share.file_scan WHERE owner LIKE '%adm%' AND is_directory = 0"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "text": {
          "titleSize": 18,
          "valueSize": 40
        },
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "orange",
                "value": null
              }
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Admin-Owned Storage",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT SUM(size) as size FROM file_share.file_scan WHERE owner LIKE '%adm%' AND is_directory = 0"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "text": {
          "titleSize": 18,
          "valueSize": 40
        },
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "orange",
                "value": null
              }
            ]
          },
          "unit": "bytes",
          "decimals": 2
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Total Unique Owners",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT uniq(splitByChar('\\\\', owner)[1]) as count FROM file_share.file_scan WHERE is_directory = 0 AND owner != ''"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "text": {
          "titleSize": 18,
          "valueSize": 40
        },
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "purple",
                "value": null
              }
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 7,
      "type": "barchart",
      "title": "Top 15 Owners/Accounts by File Count",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT owner, COUNT(*) as file_count FROM file_share.file_scan WHERE is_directory = 0 AND owner != '' GROUP BY owner ORDER BY file_count DESC LIMIT 15"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "xField": "owner",
        "showValue": "always",
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 9,
      "type": "barchart",
      "title": "Storage Distribution by Top 10 Users",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 22
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT owner, SUM(size) as total_size FROM file_share.file_scan WHERE is_directory = 0 AND owner != '' GROUP BY owner ORDER BY total_size DESC LIMIT 10"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "xField": "owner",
        "showValue": "always",
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    }
  ]
}
