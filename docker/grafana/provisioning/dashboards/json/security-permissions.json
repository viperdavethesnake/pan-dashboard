{
  "title": "Security & Permissions Analysis",
  "tags": [
    "file-share",
    "security",
    "permissions",
    "compliance"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "5m",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Files with Unknown/Empty Owner",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT COUNT(*) as count FROM file_share.file_scan WHERE (owner = '' OR owner IS NULL) AND is_directory = 0"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "text": {
          "titleSize": 16,
          "valueSize": 35
        },
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              }
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Files with SID-Only Owners",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
