  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "5m",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Files",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT COUNT(*) as value FROM file_share.file_scan WHERE is_directory = 0"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "text": {
          "titleSize": 18,
          "valueSize": 40
        },
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      }
    },
