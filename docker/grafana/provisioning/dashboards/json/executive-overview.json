    {
      "id": 5,
      "type": "barchart",
      "title": "Top 10 Owners by Storage",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT owner, SUM(size) as total_size FROM file_share.file_scan WHERE is_directory = 0 GROUP BY owner ORDER BY total_size DESC LIMIT 10"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "xField": "owner",
        "showValue": "always",
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 6,
      "type": "barchart",
      "title": "File Count by Extension (Top 15)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT if(extension = '', '(no extension)', extension) as ext, COUNT(*) as count FROM file_share.file_scan WHERE is_directory = 0 GROUP BY extension ORDER BY count DESC LIMIT 15"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "xField": "ext",
        "showValue": "always",
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 7,
      "type": "barchart",
