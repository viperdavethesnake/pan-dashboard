      "title": "Average File Size",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT AVG(size) as value FROM file_share.file_scan WHERE is_directory = 0"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "text": {
          "titleSize": 18,
          "valueSize": 40
        },
        "colorMode": "value",
